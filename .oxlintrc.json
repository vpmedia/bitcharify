{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "env": { "browser": true },
  "plugins": [
    "eslint",
    "import",
    "jsdoc",
    "jsx-a11y",
    "oxc",
    "promise",
    "react-perf",
    "react",
    "typescript",
    "unicorn"
  ],
  "categories": {
    "correctness": "error",
    "nursery": "off",
    "pedantic": "error",
    "perf": "error",
    "restriction": "error",
    "style": "warn",
    "suspicious": "error"
  },
  "rules": {
    "arrow-body-style": "off",
    "avoid-new": "off",
    "capitalized-comments": "off",
    "catch-error-name": "off",
    "consistent-type-definitions": "off",
    "consistent-type-imports": "off",
    "filename-case": "off",
    "func-style": "off",
    "group-exports": "off",
    "id-length": "off",
    "init-declarations": "off",
    "max-lines-per-function": "off",
    "max-lines": "off",
    "max-params": "off",
    "max-statements": "off",
    "no-bitwise": "off",
    "no-console": "off",
    "no-continue": "off",
    "no-default-export": "off",
    "no-inline-comments": "off",
    "no-magic-numbers": "off",
    "no-multi-assign": "off",
    "no-named-export": "off",
    "no-negated-condition": "off",
    "no-new-array": "off",
    "no-null": "off",
    "no-optional-chaining": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-rest-spread-properties": "off",
    "no-ternary": "off",
    "no-undefined": "off",
    "no-unused-vars": "off",
    "no-useless-undefined": "off",
    "no-warning-comments": "off",
    "number-literal-case": "off",
    "numeric-separators-style": "off",
    "prefer-code-point": "off",
    "prefer-default-export": "off",
    "prefer-destructuring": "off",
    "prefer-for-of": "off",
    "prefer-global-this": "off",
    "prefer-math-trunc": "off",
    "prefer-set-has": "off",
    "prefer-spread": "off",
    "require-module-specifiers": "off",
    "sort-imports": "off",
    "sort-keys": "off",
    "prefer-optional-catch-binding": "off"
  }
}
